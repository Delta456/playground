{
  "compilerOptions": {
    "module": "none",
    "target": "es5",
    "outFile": "../js/playground.bundle.js",
    "moduleResolution": "Node",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": [
    "./RunConfigurationManager",
    "./CodeRunner",
    "./Editor",
    "./Repositories",
    "./Terminal",
    "./themes",
    "./Examples",
    "./"
  ],
  "exclude": [
    "node_modules"
  ]
}
